{
    "name": "exu/bundle-skeleton",
    "version": "1.0.0",
    "type": "symfony-bundle",
    "description": "SkeletonNamespace SkeletonBundle",
    "license": "MIT",
    "autoload": {
        "psr-0": { "SkeletonNamespace\\SkeletonBundle": "" },
        "classmap": ["Composer/"]
    },
    "require": {
        "php":                           ">=5.3.3",
        "symfony/symfony":               "*@stable",
        "sensio/framework-extra-bundle": "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit":              "3.7.*",
        "behat/behat":                  "*@stable",
        "behat/symfony2-extension":     "*@stable",
        "behat/mink-extension":         "*",
        "behat/mink-browserkit-driver": "*",
        "symfony/monolog-bundle":       "*@stable"
    },
    "scripts" : {
        "post-root-package-install": [
            "SkeletonNamespace\\SkeletonBundle\\Composer\\ScriptHandler::buildProject"
        ]
    },
    "target-dir": "SkeletonNamespace/SkeletonBundle"
}
